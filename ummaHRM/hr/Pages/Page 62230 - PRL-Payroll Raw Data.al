    local procedure UpdateBasicSalary(EmpNo: Code[20]; PeriodMonth: Integer; PeriodYear: Integer; BasicSalary: Decimal; PaysNSSF: Boolean; PaysPAYE: Boolean; PaysNHIF: Boolean; PaysPension: Boolean)
    var
        PRLPayrollPeriods: Record 61081;
        PRLSalaryCard: Record 61105;
    begin
        IF ((BasicSalary <> 0) AND (EmpNo <> '')) THEN BEGIN
            PRLPayrollPeriods.RESET;
            PRLPayrollPeriods.SETRANGE("Period Month", PeriodMonth);
            PRLPayrollPeriods.SETRANGE("Period Year", PeriodYear);
            IF PRLPayrollPeriods.FIND('-') THEN BEGIN
                PRLSalaryCard.RESET;
                PRLSalaryCard.SETRANGE("Payroll Period", PRLPayrollPeriods."Date Opened");
                PRLSalaryCard.SetRange("Employee Code", EmpNo);
                IF PRLSalaryCard.FIND('-') THEN BEGIN
                    //Update
                    PRLSalaryCard."Basic Pay" := BasicSalary;
                    PRLSalaryCard."Pays NHIF" := PaysNHIF;
                    PRLSalaryCard."Pays NSSF" := PaysNSSF;
                    PRLSalaryCard."Pays PAYE" := PaysPAYE;
                    PRLSalaryCard."Pays Pension" := PaysPension;
                    PRLSalaryCard.MODIFY;
                END ELSE BEGIN
                    //Create
                    PRLSalaryCard.INIT;
                    PRLSalaryCard."Employee Code" := EmpNo;
                    PRLSalaryCard."Payroll Period" := PRLPayrollPeriods."Date Opened";
                    PRLSalaryCard."Basic Pay" := BasicSalary;
                    PRLSalaryCard."Pays NHIF" := PaysNHIF;
                    PRLSalaryCard."Pays NSSF" := PaysNSSF;
                    PRLSalaryCard."Pays PAYE" := PaysPAYE;
                    PRLSalaryCard."Pays Pension" := PaysPension;
                    PRLSalaryCard.INSERT;
                END;
            END;
        END;
    end;

