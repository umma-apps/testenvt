    local procedure CreateFirstName(FullName: Text[250]) FirstName: Text[100]
    var
        NumberofCharacters: Integer;
        NumberofSpaces: Integer;
        CountedCharacters: Integer;
    begin
        CLEAR(NumberofCharacters);
        CLEAR(NumberofSpaces);
        CLEAR(CountedCharacters);
        CLEAR(FirstName);
        IF FullName = '' THEN EXIT;
        NumberofCharacters := STRLEN(FullName);
        REPEAT
        BEGIN
            CountedCharacters += 1;
            FirstName := FirstName + COPYSTR(FullName, CountedCharacters, 1);
            IF ((COPYSTR(FullName, CountedCharacters, 1)) = ' ') THEN NumberofSpaces += 1;
        END;
        UNTIL ((CountedCharacters = NumberofCharacters) OR (NumberofSpaces = 1))
    end;

    local procedure CreateMiddleName(FullNames: Text[200]) MiddleName: Text[200]
    var
        NumberofCharacters: Integer;
        NumberofSpaces: Integer;
        CountedCharacters: Integer;
    begin
        CLEAR(NumberofCharacters);
        CLEAR(NumberofSpaces);
        CLEAR(CountedCharacters);
        CLEAR(MiddleName);
        IF FullNames = '' THEN EXIT;
        NumberofCharacters := STRLEN(FullNames);
        REPEAT
        BEGIN
            CountedCharacters += 1;
            IF NumberofSpaces = 1 THEN BEGIN
                IF ((COPYSTR(FullNames, CountedCharacters, 1)) <> ' ') THEN
                    MiddleName := MiddleName + COPYSTR(FullNames, CountedCharacters, 1);
            END;
            IF ((COPYSTR(FullNames, CountedCharacters, 1)) = ' ') THEN NumberofSpaces += 1;
        END;
        UNTIL ((CountedCharacters = NumberofCharacters) OR (NumberofSpaces = 2))
    end;

    local procedure CreateLastName(FullNamesz: Text[200]) LastNames: Text[150]
    var
        NumberofCharacters: Integer;
        NumberofSpaces: Integer;
        CountedCharacters: Integer;
    begin
        CLEAR(NumberofCharacters);
        CLEAR(NumberofSpaces);
        CLEAR(CountedCharacters);
        CLEAR(LastNames);
        IF FullNamesz = '' THEN EXIT;
        NumberofCharacters := STRLEN(FullNamesz);
        REPEAT
        BEGIN
            CountedCharacters += 1;
            IF ((COPYSTR(FullNamesz, CountedCharacters, 1)) = ' ') THEN NumberofSpaces += 1;
            IF NumberofSpaces = 2 THEN BEGIN
                IF (NumberofCharacters - CountedCharacters) > 0 THEN
                    IF ((COPYSTR(FullNamesz, CountedCharacters, (NumberofCharacters - CountedCharacters))) <> ' ') THEN
                        LastNames := LastNames + (COPYSTR(FullNamesz, CountedCharacters, (NumberofCharacters - CountedCharacters + 1)));
            END;

        END;
        UNTIL ((CountedCharacters = NumberofCharacters) OR (NumberofSpaces = 2))
    end;

