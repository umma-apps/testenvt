                    PRLEmployeeTransactions.SETRANGE(PRLEmployeeTransactions."Transaction Code", TransactionCode);
                    PRLEmployeeTransactions.SETRANGE(PRLEmployeeTransactions."Period Month", PeriodMonth);
                    PRLEmployeeTransactions.SETRANGE(PRLEmployeeTransactions."Period Year", PeriodYear);
                    PRLEmployeeTransactions.SETRANGE(PRLEmployeeTransactions."Payroll Period", PRLPayrollPeriods."Date Opened");
                    IF PRLEmployeeTransactions.FIND('-') THEN BEGIN
                        //Update
                        PRLEmployeeTransactions."Recurance Index" := 99;
                        PRLEmployeeTransactions."Transaction Name" := PRLTransactionCodes."Transaction Name";
                        IF PRLTransactionCodes."Is a Rate" THEN BEGIN
                            PRLEmployeeTransactions.Balance := 0;
                            PRLEmployeeTransactions.Amount := 0;
                            PRLEmployeeTransactions."Rate Factor" := TransactionAmount;

                        END ELSE BEGIN
                            PRLEmployeeTransactions.Balance := TransactionAmount;
                            PRLEmployeeTransactions.Amount := TransactionAmount;
                            PRLEmployeeTransactions."Rate Factor" := 0;

                        END;
                        PRLEmployeeTransactions.MODIFY;
                    END ELSE BEGIN
                        // Insert
                        PRLEmployeeTransactions.INIT;
                        PRLEmployeeTransactions."Employee Code" := EmpNo;
                        PRLEmployeeTransactions."Payroll Period" := PRLPayrollPeriods."Date Opened";
                        PRLEmployeeTransactions."Period Year" := PeriodYear;
                        PRLEmployeeTransactions."Period Month" := PeriodMonth;
                        PRLEmployeeTransactions."Transaction Code" := TransactionCode;
                        PRLEmployeeTransactions."Transaction Name" := PRLTransactionCodes."Transaction Name";
                        PRLEmployeeTransactions."Recurance Index" := 99;
                        IF PRLTransactionCodes."Is a Rate" THEN BEGIN
                            PRLEmployeeTransactions.Balance := 0;
                            PRLEmployeeTransactions.Amount := 0;
                            PRLEmployeeTransactions."Rate Factor" := TransactionAmount;


                        END ELSE BEGIN
                            PRLEmployeeTransactions.Balance := TransactionAmount;
                            PRLEmployeeTransactions.Amount := TransactionAmount;
                            PRLEmployeeTransactions."Rate Factor" := 0;

                        END;
                        IF PRLEmployeeTransactions.INSERT THEN;
                        COMMIT;
                    END;
                END ELSE
                    ERROR('Invalid Transaction Code: ' + TransactionCode);
            END ELSE
                ERROR('No period defined with Month ' + FORMAT(PeriodMonth) + ', and Year ' + FORMAT(PeriodYear));
        END;
    end;

